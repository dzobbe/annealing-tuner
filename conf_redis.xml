<bench>
	<m1>
	    <execution>local</execution>
	    <host></host>
	    <user></user>
	    <bin>run_ycsb_bench_2.sh</bin>
	    <path>/home/giovanni/</path>
	    <args>127.0.0.1 12600</args>
	    <address>127.0.0.1</address>
	    <port>12600</port>
   	</m1>
</bench> 

<annealing>
    <max_step>10000</max_step>
    <num_iter>1</num_iter>
    <min_temp>0.01</min_temp>
    <max_temp>3000</max_temp>
    <energy>throughput</energy>
    <cooling>exponential</cooling>
    <problem>default</problem>
    <version>prsa</version>
    <workers>3</workers>
</annealing>

<musl-params>
	<sthreads>
		<name>MUSL_STHREADS</name>
		<default>45</default>
		<minimum>40</minimum>
		<maximum>70</maximum>
		<step>5</step>
	</sthreads>
	<ethreads>
		<name>MUSL_ETHREADS</name>
		<default>1</default>
		<minimum>1</minimum>
		<maximum>2</maximum>
		<step>1</step>
	</ethreads>
	<ssleep>
		<name>MUSL_SSLEEP</name>
		<default>3400</default>
		<minimum>3000</minimum>
		<maximum>4000</maximum>
		<step>100</step>
	</ssleep>
	<esleep>
		<name>MUSL_ESLEEP</name>
		<default>15000</default>
		<minimum>14000</minimum>
		<maximum>18000</maximum>
		<step>1000</step>
	</esleep>
	<sspins>
		<name>MUSL_SSPINS</name>
		<default>80</default>
		<minimum>80</minimum>
		<maximum>120</maximum>
		<step>10</step>
	</sspins>
	<espins>
		<name>MUSL_ESPINS</name>
		<default>4800</default>
		<minimum>4000</minimum>
		<maximum>6000</maximum>
		<step>200</step>	
	</espins>
</musl-params>

